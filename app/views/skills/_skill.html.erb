
<% skill.each do |skill| %>
<div class="card">
  <div class="card-header">
  </div>
  <div class="card-body">
    <!-- <h5 class="card-title">Special title treatment</h5> -->
    <p class="card-text">Skills => <%= skill.skill %></p>
    <%= link_to "Edit Skill", edit_skill_path(skill), remote: true %><br>
    <%= link_to "Delete", skill_path(skill), data:{
  method: :delete,
  confirm: :"Are you sure"
} %>
  </div>
</div>
  <% end %>
