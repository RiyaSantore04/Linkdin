<!-- create post -->

<%= form_with model: @post, method: "post" do |f| %>
    
    <%= f.label :title %><br> 
    <%= f.text_field :title %><br>
    
    <%= f.label :description %><br> 
    <%= f.text_field :description%><br>
    <%= f.file_field :image %><br>
    <%= f.hidden_field :User_id, :value => current_user.id %>

<%= f.submit %>
<% end %>


